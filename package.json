{
  "name": "vkmask",
  "displayName": "vkmask",
  "publisher": "askhento",
  "repository": {
    "url": "https://github.com/Askhento/vscode_vkmask"
  },
  "description": "UI for vkontakte AR development ",
  "version": "0.0.5",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onWebviewPanel:vkmask.sidepanel"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vkmask_primary_bar",
          "title": "VK mask",
          "icon": "./res/smile-svgrepo-com.svg"
        }
      ]
    },
    "views": {
      "vkmask_primary_bar": [
        {
          "type": "webview",
          "contextualTitle": "vkmask sidepanel",
          "id": "vkmask.sidepanel",
          "name": "Sidepanel"
        }
      ]
    },
    "configuration": {
      "order": 0,
      "blah": true
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "set NODE_ENV=dev && tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "io-ts": "^2.2.20",
    "io-ts-human-reporter": "^0.1.2",
    "json-source-map": "github:ramonsnir/json-source-map#typescript-definitions"
  }
}
